        android:layout_toEndOf="@+id/textViewbusiness" />

    <TextView
        android:text="economique classe"
        android:layout_width="155dp"
        android:layout_height="40dp"
        android:id="@+id/textViewdd"
        android:layout_below="@+id/textViewbusinnesclass"
        android:layout_alignStart="@+id/textViewbusiness" />

    <TextView
        android:text="TextView"
        android:layout_width="155dp"
        android:layout_height="40dp"
        android:id="@+id/textViewclasseconomy"
        android:layout_below="@+id/textViewbusinnesclass"
        android:layout_toEndOf="@+id/textViewdd" />

</RelativeLayout>