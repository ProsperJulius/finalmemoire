        android:layout_marginEnd="21dp" />

    <TextView
        android:layout_width="155dp"
        android:layout_height="40dp"
        android:id="@+id/air2"
        android:layout_below="@+id/air1"
        android:layout_alignStart="@+id/air1" />

    <TextView
        android:layout_width="155dp"
        android:layout_height="40dp"
        android:id="@+id/date_depart_custom"
